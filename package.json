{
  "name": "yumemi-passport-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "vitest": "vitest --coverage",
    "vitest:ci": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook --coverage",
    "storybook:test:ci": "concurrently --kill-others --success=first --names='SERVER,TEST' --prefix-colors='gray,blue' 'bun storybook --quiet --no-open' 'wait-on tcp:6006 && bun storybook:test'",
    "prepare": "lefthook install && panda codegen"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@pandacss/dev": "^0.39.0",
    "@storybook/addon-a11y": "^8.0.10",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/addon-onboarding": "^8.0.10",
    "@storybook/addon-themes": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/manager-api": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@storybook/react-vite": "^8.0.10",
    "@storybook/test": "^8.0.10",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/theming": "^8.0.10",
    "@testing-library/react": "^15.0.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-istanbul": "^1.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "happy-dom": "^14.7.1",
    "lefthook": "^1.6.10",
    "prettier": "^3.2.5",
    "react-docgen-typescript": "^2.2.2",
    "storybook": "^8.0.10",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "wait-on": "^7.2.0"
  },
  "packageManager": "pnpm@8.15.6+sha256.01c01eeb990e379b31ef19c03e9d06a14afa5250b82e81303f88721c99ff2e6f",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
